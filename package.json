    "wooorm"
  ],
  "files": [
    "prose-retext.js",
    "index.js"
  ],
  "dependencies": {
    "remark-comment-config": "^4.0.1",
    "remark-github": "^6.0.0",
    "remark-lint": "^5.0.0",
    "remark-preset-lint-recommended": "^1.0.0",
    "remark-retext": "^2.0.0",
    "remark-toc": "^3.1.0",
    "remark-validate-links": "^5.0.0",
    "retext-contractions": "^1.0.1",
    "retext-english": "^3.0.0",
    "retext-indefinite-article": "^1.0.0",
    "retext-quotes": "^1.0.0",
    "retext-repeated-words": "^1.0.0",
    "retext-sentence-spacing": "^1.0.0",
    "unified": "^5.0.0"
  },
  "repository": "https://github.com/wooorm/remark-preset-wooorm",
  "bugs": "https://github.com/wooorm/remark-preset-wooorm/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "devDependencies": {
    "remark-cli": "^2.0.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "lint": "xo",
    "test": "npm run build-md && npm run lint"
